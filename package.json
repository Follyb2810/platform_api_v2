{
 "name": "platform-api",
 "version": "1.0.0",
 "scripts": {
  "dev": "nodemon server.ts",
  "dev:start": "nodemon --exec ts-node src/app.ts",
  "build": "tsc",
  "start": "node dist/app.js",
  "prisma:generate": "prisma generate",
  "prisma:migrate": "prisma migrate dev",
  "prisma:push": "prisma db push",
  "prisma:studio": "prisma studio",
  "prisma:reset": "prisma reset",
  "postinstall": "prisma generate",
  "prisma:seed": "prisma db seed",
  "seed": "npx ts-node prisma/seed.ts",
  "seed_db": "ts-node seed/seed.ts",
  "seed:db": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
  "seed:tx": "tsx prisma/seed.ts",
  "seed:esm": "ts-node-esm prisma/seed.ts"
 },
 "dependencies": {
  "@prisma/adapter-pg": "^7.0.0",
  "@prisma/client": "^7.0.0",
  "cookie-parser": "^1.4.7",
  "cors": "^2.8.5",
  "dotenv": "^16.6.1",
  "express": "^5.1.0",
  "jsonwebtoken": "^9.0.2",
  "morgan": "^1.10.1",
  "nodemailer": "^6.9.7",
  "pg": "^8.16.3",
  "swagger-jsdoc": "^6.2.8",
  "swagger-ui-express": "^5.0.1",
  "zod": "^4.3.5"
 },
 "devDependencies": {
  "@types/bcrypt": "^6.0.0",
  "@types/express": "^5.0.3",
  "@types/jsonwebtoken": "^9.0.10",
  "@types/morgan": "^1.9.10",
  "@types/node": "^20.19.30",
  "@types/nodemailer": "^7.0.1",
  "@types/pg": "^8.16.0",
  "@types/swagger-jsdoc": "^6.0.4",
  "@types/swagger-ui-express": "^4.1.8",
  "nodemon": "^3.0.1",
  "prisma": "^7.0.0",
  "ts-node": "^10.9.2",
  "tslib": "^2.8.1",
  "tsx": "^4.21.0",
  "typescript": "^5.9.3"
 }
}
